    "mocha": "^8.3.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "ts-loader": "^8.1.0",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0"
